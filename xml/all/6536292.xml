      <pair id="DDI-MedLine.d54.s3.p2" e1="DDI-MedLine.d54.s3.e1" e2="DDI-MedLine.d54.s3.e2" ddi="false" flags="True" e1_name="physostigmine" e2_name="atropine" e1_pos="27" e2_pos="31" path="27@@31@@">physostigmine@@@@conj_or@@atropine</pair> 
