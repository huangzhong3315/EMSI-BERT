   <sentence id="DDI-MedLine.d181.s0" text="Drug interaction of levothyroxine with infant colic drops.
" new_text="Drug@@interaction@@of@@levothyroxine@@with@@infant@@colic@@drops@@.@@"> 
      <entity id="DDI-MedLine.d181.s0.e0" charOffset="20-32" type="drug" text="levothyroxine" position="3"/> 
   </sentence>  
   <sentence id="DDI-MedLine.d181.s1" text="Infacol (Forest Laboratories UK, Kent, UK) is a widely available over-the-counter preparation used to relieve colic symptoms in neonates and infants. " new_text="Infacol@@(Forest@@Laboratories@@UK@@,@@Kent@@,@@UK)@@is@@a@@widely@@available@@over-the-counter@@preparation@@used@@to@@relieve@@colic@@symptoms@@in@@neonates@@and@@infants@@.@@"> 
